    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // fx_top_text 관련 요소들만 제거
            const gone = ['.fx_top_text', '.h1_mst02_chng', '.bg_full_logo', '.txt_bf_btm', '.btn_bf_btm', '.bg_full:not(#bg_full)'];
            // 혹시 남아있을지 모를 잔여 요소 제거(없으면 그냥 넘어감)
            gone.forEach(sel => document.querySelectorAll(sel).forEach(el => el.remove()));
        });
    </script>
    
