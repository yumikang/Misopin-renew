<style>
/* 사이드 메뉴 드롭다운 스타일 */
.left_menu .l_nav_sub {
    display: none !important;
    overflow: hidden;
    transition: all 0.3s ease;
}

.left_menu .has-submenu.active .l_nav_sub {
    display: block !important;
}
</style>
<script>
// 모바일 사이드 메뉴 드롭다운 기능
document.addEventListener('DOMContentLoaded', function() {
    // 메인 링크 클릭 시 서브메뉴 토글
    const hasSubmenuLinks = document.querySelectorAll('.left_menu .has-submenu > a');
    hasSubmenuLinks.forEach(link => {
        link.addEventListener('click', function(e) {
            e.preventDefault();
            const parentLi = this.closest('.has-submenu');
            
            // 다른 열린 메뉴들을 닫기
            document.querySelectorAll('.left_menu .has-submenu.active').forEach(item => {
                if (item !== parentLi) {
                    item.classList.remove('active');
                }
            });
            
            // 현재 메뉴 토글
            parentLi.classList.toggle('active');
        });
    });
});
</script>
